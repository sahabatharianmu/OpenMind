
OPENMIND_APPLICATION_NAME=OpenMind
OPENMIND_APPLICATION_VERSION=0.0.1
OPENMIND_APPLICATION_ENVIRONMENT=development

OPENMIND_SERVER_HOST=0.0.0.0
OPENMIND_SERVER_PORT=8080
OPENMIND_SERVER_READ_TIMEOUT=30s
OPENMIND_SERVER_WRITE_TIMEOUT=30s
OPENMIND_SERVER_IDLE_TIMEOUT=60s
OPENMIND_SERVER_EXIT_TIMEOUT=60s
OPENMIND_SERVER_MODE=development

OPENMIND_DATABASE_HOST=localhost
OPENMIND_DATABASE_PORT=5432
OPENMIND_DATABASE_USER=user
OPENMIND_DATABASE_PASSWORD=password
OPENMIND_DATABASE_DB_NAME=openmind
OPENMIND_DATABASE_SSL_MODE=disable
OPENMIND_DATABASE_MAX_OPEN_CONNS=25
OPENMIND_DATABASE_MAX_IDLE_CONNS=5

OPENMIND_REDIS_HOST=localhost
OPENMIND_REDIS_PORT=6379
OPENMIND_REDIS_PASSWORD=""
OPENMIND_REDIS_DB=0

OPENMIND_JWT_SECRET_KEY=your-secret-key-change-this-in-production
OPENMIND_JWT_ACCESS_TOKEN_EXPIRY=15m
OPENMIND_JWT_REFRESH_TOKEN_EXPIRY=168h

OPENMIND_SECURITY_CORS_ALLOW_ORIGINS=http://localhost:3000
OPENMIND_SECURITY_CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS
OPENMIND_SECURITY_CORS_ALLOW_HEADERS=Authorization,Content-Type
OPENMIND_SECURITY_CORS_MAX_AGE=43200
OPENMIND_SECURITY_RATE_LIMIT_REQUESTS=100
OPENMIND_SECURITY_RATE_LIMIT_WINDOW=1m
OPENMIND_SECURITY_JWT_SECRET_KEY=your-secret-key-change-this-in-production
OPENMIND_SECURITY_JWT_ACCESS_EXPIRY=15m
OPENMIND_SECURITY_JWT_REFRESH_EXPIRY=168h
OPENMIND_SECURITY_PASSWORD_MIN_LENGTH=8
OPENMIND_SECURITY_PASSWORD_REQUIRE_UPPER=true
OPENMIND_SECURITY_PASSWORD_REQUIRE_LOWER=true
OPENMIND_SECURITY_PASSWORD_REQUIRE_NUMBER=true
OPENMIND_SECURITY_PASSWORD_REQUIRE_SPECIAL=true
OPENMIND_SECURITY_SESSION_TIMEOUT=24h
OPENMIND_SECURITY_SESSION_SECURE=false
OPENMIND_SECURITY_SESSION_HTTPONLY=true
OPENMIND_SECURITY_MAX_FILE_SIZE=10485760
OPENMIND_SECURITY_ALLOWED_FILE_TYPES=.jpg,.jpeg,.png,.pdf,.xlsx
OPENMIND_SECURITY_API_KEY_HEADER=X-API-Key
OPENMIND_SECURITY_ENABLE_API_KEY=false
OPENMIND_SECURITY_ENABLE_CSRF=true
OPENMIND_SECURITY_ENABLE_CSP=true
OPENMIND_SECURITY_DB_MAX_OPEN_CONNS=25
OPENMIND_SECURITY_DB_MAX_IDLE_CONNS=5
OPENMIND_SECURITY_DB_CONN_MAX_LIFETIME=1h
OPENMIND_SECURITY_ENABLE_SECURITY_LOGGING=true
OPENMIND_SECURITY_LOG_SECURITY_EVENTS=true
OPENMIND_SECURITY_ENCRYPTION_KEY=your-encryption-key-32-bytes-long
OPENMIND_SECURITY_HASH_COST=10
OPENMIND_SECURITY_MAX_REQUEST_SIZE=10485760
OPENMIND_SECURITY_REQUEST_TIMEOUT=30s
OPENMIND_SECURITY_MAX_REQUEST_PER_IP=1000
OPENMIND_SECURITY_MAX_REQUEST_PER_USER=100

OPENMIND_EMAIL_PROVIDER=smtp
OPENMIND_EMAIL_FROM_EMAIL=noreply@mail.com
OPENMIND_EMAIL_FROM_NAME=OpenMind
OPENMIND_EMAIL_SMTP_HOST=smtp.gmail.com
OPENMIND_EMAIL_SMTP_PORT=587
OPENMIND_EMAIL_SMTP_USERNAME=your-email@gmail.com
OPENMIND_EMAIL_SMTP_PASSWORD=your-app-password
OPENMIND_EMAIL_SMTP_TLS=true
OPENMIND_EMAIL_SENDGRID_API_KEY=your-sendgrid-api-key
OPENMIND_EMAIL_AWS_ES_REGION=us-east-1
OPENMIND_EMAIL_AWS_ES_ACCESS_KEY=your-aws-access-key
OPENMIND_EMAIL_AWS_ES_SECRET_KEY=your-aws-secret-key

OPENMIND_SMS_PROVIDER=local
OPENMIND_SMS_TWILIO_ACCOUNT_SID=your-account-sid
